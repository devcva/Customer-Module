



<div class="container mt-5" style="width: 25%;">
    <form>
        <div class="form-group">
            <label for="name">Customer Name</label>
            <Input @type="text" @class="form-control" @id="inpname" @value={{this.model.name}} />
        </div>
        <div class="form-group">
            <label for="displayname">Display Name</label>
            <Input @type="text" @class="form-control" @id="inpdisplayname" @value={{this.model.displayname}} />
        </div>
        <div class="form-group">
           <label for="phonenumber">Phone Number</label>
            <Input @type="number" @class="form-control" @id="inpphonenumber" @value={{this.model.phonenumber}} />
        </div>
        <button type="button" class="btn btn-primary" {{action 'addCus' this.model.name this.model.displayname this.model.phonenumber this.model}}>Save</button>
    </form>
</div>



